{
  "name": "the-retro-retro-game",
  "version": "0.1.0",
  "description": "A retro-feeling retro(spective) game that makes it easier for teams to bond, be it in person or online.",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "client-install": "cd frontend && npm install",
    "client-build": "cd frontend && npm run build",
    "client-start": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "build": "cd frontend && npm run build"
  },
  "dependencies": {
    "@dotlottie/player-component": "^2.7.12",
    "axios": "^1.7.9",
    "express": "^4.21.2",
    "react-lottie-player": "^2.1.0",
    "ws": "^8.18.0",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object":"7.21.11",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  },
  "license": "Apache-2.0"
}
